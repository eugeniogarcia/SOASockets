<?xml version="1.0" encoding="windows-1252" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:util="http://www.springframework.org/schema/util"
       xmlns:jee="http://www.springframework.org/schema/jee" xmlns:lang="http://www.springframework.org/schema/lang"
       xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:sca="http://xmlns.oracle.com/weblogic/weblogic-sca" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
     
  <!--Spring Bean definitions go here-->
  <bean id="beanPersonas" class="euge.soa.example.beans.beanPersonas">
    <constructor-arg ref="servicioPersona" index="0"/>
    <constructor-arg index="1" type="java.lang.String" value="locahost"/>
     <constructor-arg index="2" value="12116" type="java.lang.Integer"/>
      <constructor-arg index="3" type="java.lang.Integer" value="12116"/>
  </bean>
  <sca:service type="euge.soa.example.interfaces.gestionSocket" name="gestionSockets" target="beanPersonas">
  </sca:service>
  <sca:reference type="euge.soa.example.interfaces.servicioPersona" name="servicioPersona"/>
</beans>
